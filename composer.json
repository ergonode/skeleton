{
    "name": "ergonode/skeleton",
    "type": "project",
    "license": "OSL-3",
    "description": "Ergonode skeleton project to create backend application",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "ergonode/account": "^0.10",
        "ergonode/api": "^0.10",
        "ergonode/attribute": "^0.10",
        "ergonode/authentication": "^0.10",
        "ergonode/category": "^0.10",
        "ergonode/channel": "^0.10",
        "ergonode/comment": "^0.10",
        "ergonode/completeness": "^0.10",
        "ergonode/condition": "^0.10",
        "ergonode/core": "^0.10",
        "ergonode/designer": "^0.10",
        "ergonode/editor": "^0.10",
        "ergonode/es": "^0.10",
        "ergonode/exporter": "^0.10",
        "ergonode/exporter-file": "^0.10",
        "ergonode/fixture": "^0.10",
        "ergonode/grid": "^0.10",
        "ergonode/importer": "^0.10",
        "ergonode/migration": "^0.10",
        "ergonode/mailer": "^0.10",
        "ergonode/multimedia": "^0.10",
        "ergonode/notification": "^0.10",
        "ergonode/product": "^0.10",
        "ergonode/product-collection": "^0.10",
        "ergonode/reader": "^0.10",
        "ergonode/segment": "^0.10",
        "ergonode/shared-kernel": "^0.10",
        "ergonode/transformer": "^0.10",
        "ergonode/value": "^0.10",
        "ergonode/workflow": "^0.10",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/messenger": "4.4.10",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.7",
        "symfony/yaml": "4.4.*"
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
